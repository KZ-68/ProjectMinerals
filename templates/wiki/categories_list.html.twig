{% extends 'base.html.twig' %}

{% block title %}Categories List{% endblock %}

{% block body %}

<h1>Categories List</h1>

<div class="list-wrapper">
    <div>
        <ul>
            {% for category in categories %}
                <li><a>{{category.name}}</a></li>
            {% endfor %}
        </ul>
    </div>
</div>

{% endblock %}
